#include <iostream>
#include <algorithm>

using namespace std;

void fixArray(int arr[], int n){
    for(int i=0;i<n;i++){
        if(arr[i] != -1 && arr[i]!= i){

            }
        }
    }
}

int main() {
    // Write C++ code here
    int n, arr[] = { -1, -1, 6, 1, 9, 3, 2, -1, 4, -1 };
    n = sizeof(arr) / sizeof(arr[0]);
    
    fixArray(arr, n);

    return 0;
}
